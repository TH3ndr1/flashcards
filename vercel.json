{
  "functions": {
    "app/api/extract-pdf/route.ts": {
      "memory": 3008,
      "maxDuration": 90
    }
  },
  "routes": [
    {
      "src": "/api/extract-pdf(.*)",
      "dest": "/api/extract-pdf",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "bodyParser": {
    "sizeLimit": "30mb"
  }
} 